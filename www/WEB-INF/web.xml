<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
                      http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
  version="3.0"
  metadata-complete="true">


    <description>
      MyCloud - private netdisk
    </description>
    <display-name>MyCloud</display-name>

    <servlet>
        <servlet-name>Main</servlet-name>
        <servlet-class>com.Discover255.MyCloud.Main</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HS</servlet-name>
        <servlet-class>com.Discover255.MyCloud.HTTPServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>Main</servlet-name>
        <url-pattern>/MC</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HS</servlet-name>
        <url-pattern>/fuck</url-pattern>
    </servlet-mapping>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>